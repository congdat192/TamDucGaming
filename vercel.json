{
  "crons": [
    {
      "path": "/api/cron/notifications",
      "schedule": "0 1 * * *"
    }
  ],
  "headers": [
    {
      "source": "/leaderboard(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
